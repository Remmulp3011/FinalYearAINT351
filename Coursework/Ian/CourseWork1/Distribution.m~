samples = 1000;

dataUniform = rand(samples,1);

nbins = 25;

message = sprintf('dataUniform Matric size %d x %d', size(dataUniform,1), size(dataUniform,2));
disp(message);

figure
subplot(1,2,1);
hold on
h = plot(dataUniform,'b.');
set(h,'linewidth',3);
title('dataUniform raw data plot');
xlabel('Data point number');
ylabel('Data point value');

subplot(1,2,2);
hold on
i = hist(dataUniform,);
set(i,'linewidth',3);
title('dataUniform raw data plot');
xlabel('Data point number');
ylabel('Data point value');


