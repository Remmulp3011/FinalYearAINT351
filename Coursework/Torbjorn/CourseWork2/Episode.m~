function Episode
%EPISODE Summary of this function goes here
%   Detailed explanation goes here
global endTrialState;
global qTable;
global TrialNumber;
stepNumberStore=[];

stepNumber = 1;
initQ();

while TrialNumber <= 50
StartingState();

while endTrialState ~= 2
    state = endTrialState;
    stepNumber = stepNumber + 1;
    eGreedyActionSelection(qTable,state);
end
stepNumberStore(1,TrialNumber) = stepNumber;
TrialNumber = TrialNumber + 1;
end

disp('Goal state reached. Number of steps = ');
disp(stepNumber);
disp(stepNumberStore);
disp(qTable);

end